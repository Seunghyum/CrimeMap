{
  "name": "crime-map-backend",
  "version": "0.3.0",
  "description": "express-es6-rest-api 레포로변경. Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "developit/express-es6-rest-api",
  "author": "Jason Miller <jason@developit.ca>",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^2.8.1",
    "bluebird": "^3.5.5",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.4.2",
    "graphql-tools": "^4.0.5",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "morgan": "^1.8.0",
    "mysql": "^2.17.1",
    "mysql2": "^1.6.5",
    "resource-router-middleware": "^0.6.0",
    "sequelize": "^5.9.0",
    "type-graphql": "^0.17.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/express-graphql": "^0.8.0",
    "@types/gulp": "^4.0.6",
    "@types/lodash": "^4.14.136",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.7.36",
    "@types/mysql": "^2.15.6",
    "@types/node": "^12.6.8",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "csv-parse": "^4.4.3",
    "gulp": "^4.0.2",
    "iconv-lite": "^0.5.0",
    "nodemon": "^1.9.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.5.3"
  }
}
