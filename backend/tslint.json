{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-airbnb",
        "tslint-config-prettier",
        "tslint-eslint-rules"
    ],
    "rules": {
        "variable-name": false,
        "function-name": false,
        "semicolon": true,
        "whitespace": [true, "check-branch", "check-operator", "check-typecast"]
    }
}
