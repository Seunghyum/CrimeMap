{
  "name": "crime-map",
  "version": "1.0.0",
  "author": "문승현 <antmgu@gmail.com>",
  "license": "MIT",
  "engines": {
    "yarn": "1.x"
  },
  "scripts": {
    "install": "yarn install:backend && yarn install:frontend",
    "install:backend": "cd backend && yarn install",
    "install:frontend": "cd frontend && yarn install",
    "add": "yarn add:backend && yarn add:frontend",
    "add:backend": "cd backend && yarn add",
    "add:frontend": "cd frontend && yarn add",
    "remove": "yarn remove:backend && yarn remove:frontend",
    "remove:backend": "cd backend && yarn remove",
    "remove:frontend": "cd frontend && yarn remove",
    "build": "yarn build:backend && yarn build:frontend",
    "build:backend": "cd backend && yarn build",
    "build:frontend": "cd frontend && yarn build",
    "start": "concurrently \"yarn start:backend\" \"yarn start:frontend\"",
    "start:backend": "cd backend && yarn start",
    "start:frontend": "cd frontend && yarn start",
    "dev": "concurrently \"yarn dev:backend\" \"yarn dev:frontend\"",
    "dev:backend": "cd backend && yarn dev",
    "dev:frontend": "cd frontend && yarn dev",
    "download-config": "aws s3 cp s3://crime-config ./config --recursive",
    "upload-config": "aws s3 cp ./config s3://crime-config --recursive"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  },
  "dependencies": {
    "bootstrap": "^4.3.1"
  }
}
