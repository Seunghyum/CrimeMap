{
  "defaultSeverity": "error",
  "extends": ["tslint-config-airbnb", "tslint-react", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "variable-name": false,
    "function-name": false,
    "jsx-no-lambda": false,
    "import-name": [false, {
      "react": "React"
    }],
    "semicolon": [true, "never"]
  },
  "rulesDirectory": [],
  "additionalItems": false,
  "no-trailing-whitespace": false,
  "no-boolean-literal-compare": false
}