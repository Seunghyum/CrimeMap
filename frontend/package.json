{
  "name": "crime-map-front",
  "version": "2.0.0",
  "description": "ts-react-parcel 레포에서 가져옴. Typescript with React, using parcel bundler",
  "repository": {
    "type": "git",
    "url": "https://github.com/adhrinae/ts-react-parcel"
  },
  "author": "",
  "main": "./index.tsx",
  "scripts": {
    "dev": "concurrently -r \"npm:type-check:watch\" \"npm:start\"",
    "start": "parcel ./index.html --port 8080",
    "build": "npm run type-check && parcel build ./index.html --public-url '/'",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettify"
    }
  },
  "engines": {
    "node": ">= 8.11.0",
    "npm": ">= 6.0.0"
  },
  "keywords": [
    "Typescript",
    "React",
    "Parcel"
  ],
  "dependencies": {
    "react": "^16.8.6",
    "react-async-script": "^1.0.1",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^11.13.6",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "concurrently": "^4.1.0",
    "husky": "^1.3.1",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.17.0",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "^6.1.2",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.4"
  }
}
